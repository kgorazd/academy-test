<p id="notice"><%= notice %></p>
<h1>Listing Cities</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Short name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @cities.each do |city| %>
      <%= render city %>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New City', new_city_path %>
